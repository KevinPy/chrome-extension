{
  "name": "Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A boilerplate for your Chrome Extension",
  "icons": {
    "128": "assets/img/icons/icon-128.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/img/icons/icon-19.png",
      "38": "assets/img/icons/icon-19@2x.png",
      "76": "assets/img/icons/icon-19@3x.png"
    },
    "default_popup": "popup.html"
  }
}


